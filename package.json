{
  "private": true,
  "lint-staged": {
    "*.{ts,js,vue}": [
      "pnpm format",
      "pnpm lint:fix"
    ],
    "*.md": [
      "pnpm format",
      "pnpm mdlint:fix"
    ]
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
